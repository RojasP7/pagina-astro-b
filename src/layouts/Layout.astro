---
const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="description" content="Astro description" />
    <meta name="viewport" content="width=device-width" />
    <link
      rel="icon"
      type="image/svg+xml"
      href="https://http2.mlstatic.com/frontend-assets/ml-web-navigation/ui-navigation/5.21.22/mercadolibre/favicon.svg"
    />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap"
      rel="stylesheet"
    />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
  </head>
  <body class="bg-[#f2f2f2] m-0 p-0">
    <header class="flex justify-center h-20 bg-[#ffe600] pt-3">
      <div>
        <div class="w-[1200px] flex justify-between">
          <div class="w-[120px]">
            <img
              src="https://http2.mlstatic.com/frontend-assets/ml-web-navigation/ui-navigation/6.6.73/mercadolibre/logo_large_25years_v2.png"
              alt=""
              class="w-[130px]"
            />
          </div>
          <div class="w-[700px] flex justify-center">
            <input
              type="text"
              placeholder="Buscar productos, marcas y mÃ¡s..."
              class="w-[80%] h-10 pl-[15px] pr-2 bg-white text-[#636363] text-sm font-[Roboto] font-[380] shadow-[3px_1px_2px_rgba(0,0,0,0.1)] rounded-l-md outline-none"
            />
            <div class="h-10 w-px bg-white flex items-center">
              <span class="w-px h-5 bg-[#b1b1b1]"></span>
            </div>
            <button
              class="flex items-center justify-center w-8 h-10 bg-white rounded-r-md shadow-[2px_1px_2px_rgba(0,0,0,0.1)] cursor-pointer"
            >
              <svg
                xmlns="http://www.w3.org/2000/svg"
                fill="none"
                viewBox="0 0 24 24"
                stroke-width="1"
                stroke="gray"
                class="size-5"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  d="m21 21-5.197-5.197m0 0A7.5 7.5 0 1 0 5.196 5.196a7.5 7.5 0 0 0 10.607 10.607Z"
                ></path>
              </svg>
            </button>
          </div>
          <div>
            <a href="https://www.disneyplus.com/es-us" target="_blank">
              <img
                src="https://http2.mlstatic.com/D_NQ_873803-MLA78739315606_092024-OO.webp"
                alt=""
                class="w-[350px] h-10"
              />
            </a>
          </div>
        </div>
        <div class="flex w-[1200px] justify-between">
          <button
            class="border border-[#ffe600] hover:border-[rgba(132,132,132,0.71)] rounded flex p-1 h-11"
          >
            <svg
              class="w-7"
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke-width="0.6"
              stroke="currentColor"
              class="size-6"
            >
              <path
                stroke-linecap="round"
                stroke-linejoin="round"
                d="M15 10.5a3 3 0 1 1-6 0 3 3 0 0 1 6 0Z"></path>
              <path
                stroke-linecap="round"
                stroke-linejoin="round"
                d="M19.5 10.5c0 7.142-7.5 11.25-7.5 11.25S4.5 17.642 4.5 10.5a7.5 7.5 0 1 1 15 0Z"
              ></path>
            </svg>
            <div class="font-[Roboto] text-left">
              <h3 class="font-[300] text-xs text-[#535353]">Ingresa tu</h3>
              <h3 class="font-[370] text-sm text-black">Ubicacion</h3>
            </div>
          </button>
          <div
            class="flex items-center gap-4 font-[Roboto] font-[370] text-sm mt-2"
          >
            <div class="relative group inline-block">
              <button class="flex mt-3 gap-1"
                >Categorias <svg
                  xmlns="http://www.w3.org/2000/svg"
                  fill="none"
                  viewBox="0 0 24 24"
                  stroke-width="0.8"
                  stroke="currentColor"
                  class="size-4 mt-1"
                >
                  <path
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    d="m19.5 8.25-7.5 7.5-7.5-7.5"></path>
                </svg>
                <div class="w-64 group-hover:block hidden top-8 bg-slate-800 absolute pt-4 text-white hover:cursor-pointer hover:bg-blue-600 rounded">
                  <div class="absolute rotate-45 -top-1 left-[74px] bg-slate-800 size-4"></div>
                    opcion1
                </div>
              </button>
            </div>
            <button class="mt-3">Ofertas</button>
            <button class="mt-3">Cupones</button>
            <button class="relative flex flex-col items-center h-8">
              <p class="mt-3">Supermercado</p>
            </button>
            <button class="mt-3">Moda</button>
            <button class="mt-3">Vender</button>
            <button class="mt-3">Ayuda / PQR</button>
          </div>
          <div
            class="flex items-center gap-4 font-[Roboto] font-[370] text-sm mt-2"
          >
            <button class="flex mt-3 gap-1"
              ><svg
                xmlns="http://www.w3.org/2000/svg"
                fill="none"
                viewBox="0 0 24 24"
                stroke-width="1"
                stroke="currentColor"
                class="size-5"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  d="M17.982 18.725A7.488 7.488 0 0 0 12 15.75a7.488 7.488 0 0 0-5.982 2.975m11.963 0a9 9 0 1 0-11.963 0m11.963 0A8.966 8.966 0 0 1 12 21a8.966 8.966 0 0 1-5.982-2.275M15 9.75a3 3 0 1 1-6 0 3 3 0 0 1 6 0Z"
                ></path>
              </svg>
              Crea tu cuenta
            </button>
            <button class="mt-3">Ingresa</button>
            <button class="mt-3">Mis compras</button>
            <button
              ><svg
                xmlns="http://www.w3.org/2000/svg"
                fill="none"
                viewBox="0 0 24 24"
                stroke-width="0.9"
                stroke="currentColor"
                class="size-5 mt-3"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  d="M2.25 3h1.386c.51 0 .955.343 1.087.835l.383 1.437M7.5 14.25a3 3 0 0 0-3 3h15.75m-12.75-3h11.218c1.121-2.3 2.1-4.684 2.924-7.138a60.114 60.114 0 0 0-16.536-1.84M7.5 14.25 5.106 5.272M6 20.25a.75.75 0 1 1-1.5 0 .75.75 0 0 1 1.5 0Zm12.75 0a.75.75 0 1 1-1.5 0 .75.75 0 0 1 1.5 0Z"
                ></path>
              </svg>
            </button>
          </div>
        </div>
      </div>
    </header>
    <div class="h-[320px] bg-gradient-to-b from-[#ffe600] to-[#f2f2f2]"></div>
    <div class="mt-[-80px] h-[270px] flex gap-3 max-w-[1200px] mx-auto">
      <div
        class="flex-grow bg-white rounded-lg group hover:cursor-pointer px-4 w-[130px] py-4 font-[Roboto] max-w-44"
      >
        <h4 class="font-normal">Ingresa a tu cuenta</h4>
        <img
          class="w-[100px] ml-5 mb-2"
          src="https://http2.mlstatic.com/frontend-assets/homes-palpatine/dynamic-access-desktop/registration-da.svg"
          alt=""
        />
        <h5
          class="group-hover:text-blue-500 flex flex-wrap font-light text-[14px] text-center"
        >
          Disfruta de compras y ofertas sin limites
        </h5>
        <div class="bg-blue-100 mt-10 text-blue-600 rounded text-[12px] font-medium h-6 text-center">Ingresa a tu cuenta</div>
      </div>
      <div class="flex-grow bg-white rounded-lg"><h4>tarjeta 2</h4></div>
      <div class="flex-grow bg-white rounded-lg">
        <h4>tarjeta 3</h4>
      </div>
      <div class="flex-grow bg-white rounded-lg">
        <h4>tarjeta 4</h4>
      </div>
      <div class="flex-grow bg-white rounded-lg"><h4>tarjeta 5</h4></div>
      <div class="flex-grow bg-white rounded-lg"><h4>tarjeta 6</h4></div>
    </div>
    <div
      class="group bg-blue-500 text-white py-2 px-4 inline-flex rounded hover:cursor-pointer hover:bg-blue-700 transition-colors duration-300 items-center"
    >
      <svg
        xmlns="http://www.w3.org/2000/svg"
        fill="none"
        viewBox="0 0 24 24"
        stroke-width="1.5"
        stroke="currentColor"
        class="size-4 mt-[4px] mr-1 group-hover:animate-spin"
      >
        <path
          stroke-linecap="round"
          stroke-linejoin="round"
          d="M16.023 9.348h4.992v-.001M2.985 19.644v-4.992m0 0h4.992m-4.993 0 3.181 3.183a8.25 8.25 0 0 0 13.803-3.7M4.031 9.865a8.25 8.25 0 0 1 13.803-3.7l3.181 3.182m0-4.991v4.99"
        ></path>
      </svg>
      Presioname!
      <svg
        xmlns="http://www.w3.org/2000/svg"
        fill="none"
        viewBox="0 0 24 24"
        stroke-width="1.5"
        stroke="currentColor"
        class="size-4 mt-[4px] ml-1"
      >
        <path
          stroke-linecap="round"
          stroke-linejoin="round"
          d="M13.5 4.5 21 12m0 0-7.5 7.5M21 12H3"></path>
      </svg>
    </div>
    <!-- <slot /> -->
  </body>
</html>
